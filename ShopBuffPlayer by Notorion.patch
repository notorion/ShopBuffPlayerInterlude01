+		{
+			player.sendMessage("You can only advance to level ");
+			return "npc_level_change_not_allowed.html";
+		}
+		
+		if (getQuestItemsCount(player, ADENA) >= LEVEL_UP_COST)
+		{
+			takeItems(player, ADENA, LEVEL_UP_COST);
+			setPlayerLevel(player, level);
+			
+			npc.broadcastSay(ChatType.NPC_GENERAL, "Congratulations! " + player.getName() + " now it is Level " + level);
+			return "npc_level_up_success.html";
+		}
+		
+		return "npc_no_adena_level.html";
+	}
+	
+	private void setPlayerLevel(Player player, int level)
+	{
+		if ((level >= 1) && (level <= 120))
+		{
+			long exp = player.getStat().getExpForLevel(level);
+			player.getStat().addExp(exp - player.getExp());
+			player.broadcastUserInfo();
+		}
+	}
+	
+	public static void main(String[] args)
+	{
+		new ShopBuffPlayer();
+	}
+}
