+	private boolean checkAreaSkillTargets(Player player, Skill skill)
+	{
+		for (Creature target : World.getInstance().getVisibleObjectsInRange(player, Creature.class, skill != null ? skill.getEffectRange() : 200))
+		{
+			if ((target instanceof Player) && (target != player))
+			{
+				Player targetPlayer = (Player) target;
+				if ((player.isInDuel() && targetPlayer.isInDuel()) || (player.isInSiege() && targetPlayer.isInSiege()) || (player.isInOlympiadMode() && targetPlayer.isInOlympiadMode()) || (player.atWarWith(targetPlayer)) || (player.isInCombat() && target.isInCombat()))
+				{
+					
+					for (WeakReference<Creature> weakRef : player.getAttackByList())
+					{
+						if (weakRef.get() == target)
+						{
+							return true;
+						}
+					}
+					
+					for (WeakReference<Creature> weakRef : target.getAttackByList())
+					{
+						if (weakRef.get() == player)
+						{
+							return true;
+						}
+					}
+				}
+			}
+		}
+		return false;
+	}
