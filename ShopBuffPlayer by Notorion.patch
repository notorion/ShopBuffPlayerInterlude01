+	};
+	
+	private static final int SUPPORT_SKILL_1_ID = 888861;
+	private static final int SUPPORT_SKILL_2_ID = 888860;
+	private static final double TELEPORT_DISTANCE = 500;
+	private static final int CHECK_INSTANCE_DELAY = 1000; // Interval to check the player's instance (in milliseconds).
+	
+	private static class DaySchedule
+	{
+		int dayOfWeek;
+		boolean enabled;
+		int startHour;
+		int startMinute;
+		int endHour;
+		int endMinute;
+		
+		public DaySchedule(int dayOfWeek, boolean enabled, int startHour, int startMinute, int endHour, int endMinute)
+		{
+			this.dayOfWeek = dayOfWeek;
+			this.enabled = enabled;
+			this.startHour = startHour;
+			this.startMinute = startMinute;
+			this.endHour = endHour;
+			this.endMinute = endMinute;
+		}
+	}
+	
+	private static DaySchedule[] SCHEDULE;
+	private long lastReloadTime = 0;
+	private static final long RELOAD_INTERVAL = 1 * 60 * 1000; // 1 minute in milliseconds
+	
+	private final Map<Integer, Npc> playerNpcMap = new HashMap<>();
+	private final Map<Integer, Boolean> buffActive = new HashMap<>();
+	private final Map<Integer, Long> _reuseTime = new HashMap<>();
+	private final Map<Integer, Long> blockTimer = new HashMap<>();
+	private static final Map<String, Integer> DAY_OF_WEEK_MAP = new HashMap<>();
+	static
+	{
+		DAY_OF_WEEK_MAP.put("MONDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("TUESDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("WEDNESDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("THURSDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("FRIDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("SATURDAY", Calendar.MONDAY);
+		DAY_OF_WEEK_MAP.put("SUNDAY", Calendar.MONDAY);
+	}
+	
+	private final Random _random = new Random();
+	
+	public BuffFollow()
+	{
+		addStartNpc(SHOP_NPC);
+		for (int npcId : FOLLOW_BUFF_NPCS)
+		{
+			addTalkId(SHOP_NPC, npcId);
+		}
+		for (int npcId : FOLLOW_BUFF_NPCS)
+		{
+			addFirstTalkId(SHOP_NPC, npcId);
+		}
+		addSpawnId(FOLLOW_BUFF_NPCS);
+		addAttackId(FOLLOW_BUFF_NPCS);
+		
+		startQuestTimer("checkSchedule", 60000, null, null, true);
+		startQuestTimer("checkDistance", 2000, null, null, true);
+		startQuestTimer("checkInstance", CHECK_INSTANCE_DELAY, null, null, true);
+		loadScheduleFromFile();
+		if (SCHEDULE == null)
+		{
+			SCHEDULE = new DaySchedule[0];
+		}
+	}
+	
+	private void checkNpcSchedule()
+	{
+		Calendar calendar = Calendar.getInstance();
+		int dayOfWeek = calendar.get(Calendar.DAY_OF_WEEK);
+		int hourOfDay = calendar.get(Calendar.HOUR_OF_DAY);
+		int minuteOfHour = calendar.get(Calendar.MINUTE);
+		LOGGER.info("Verifying Schedule: Day " + dayOfWeek + ", hour " + hourOfDay + ":" + minuteOfHour);
+		long currentTime = System.currentTimeMillis();
+		if ((currentTime - lastReloadTime) > RELOAD_INTERVAL)
+		{
+			LOGGER.info("Recharging TXT scheduling file ...");
+			loadScheduleFromFile();
+			lastReloadTime = currentTime;
+		}
+		for (DaySchedule daySchedule : SCHEDULE)
+		{
+			if (daySchedule.dayOfWeek == dayOfWeek)
+			{
+				boolean isActive = daySchedule.enabled && ((hourOfDay > daySchedule.startHour) || ((hourOfDay == daySchedule.startHour) && (minuteOfHour >= daySchedule.startMinute))) && ((hourOfDay < daySchedule.endHour) || ((hourOfDay == daySchedule.endHour) && (minuteOfHour < daySchedule.endMinute)));
+				LOGGER.info("Schedule for the day " + dayOfWeek + ": " + (isActive ? "Active" : "Inactive"));
+				
+				if (isActive)
+				{
+					LOGGER.info("NPC activated for the day " + dayOfWeek);
+				}
+				else
+				{
+					for (Player p : World.getInstance().getPlayers())
+					{
+						Npc buffNpc = playerNpcMap.get(p.getObjectId());
+						if (buffNpc != null)
+						{
+							cancelBuffs(p);
+							p.sendMessage("The support NPC has been deactivated.");
+							LOGGER.info("NPC Deactivated to the player " + p.getName());
+						}
+					}
+				}
+				break;
+			}
+		}
+	}
+	
+	public void onNpcTeleported(OnNpcTeleport event)
+	{
+		Npc buffNpc = event.getNpc();
+		WorldObject target = buffNpc.getTarget();
+		
+		if (target instanceof Player)
+		{
+			Player player = (Player) target;
+			int heading = player.getHeading();
+			double radian = Math.toRadians(heading);
+			int offsetX = (int) (Math.cos(radian) * -500); // distance behind the player
+			int offsetY = (int) (Math.sin(radian) * -500);
+			int npcX = player.getX() + offsetX;
+			int npcY = player.getY() + offsetY;
+			int npcZ = player.getZ();
+			
+			buffNpc.teleToLocation(npcX, npcY, npcZ);
+		}
+	}
+	
+	@Override
+	public String onSpawn(Npc npc)
+	{
+		if (Arrays.stream(FOLLOW_BUFF_NPCS).anyMatch(id -> id == npc.getId()))
+		{
+			startQuestTimer("CHECK_DISTANCE", 2000, npc, null, true);
+		}
+		return super.onSpawn(npc);
+	}
+	
+	@Override
+	public String onEvent(String event, Npc npc, Player player)
+	{
+		switch (event)
+		{
+			case "follow_buffs":
+				if (_reuseTime.containsKey(player.getObjectId()) && (_reuseTime.get(player.getObjectId()) > System.currentTimeMillis()))
+				{
+					player.sendMessage("You should wait just before requesting the buffs again.");
+					return null;
+				}
+				
+				if (!isNpcActive())
+				{
+					player.sendMessage("Follow Player Support is not currently available.");
+					player.sendPacket(new SystemMessage(13594)); // It is not available for interlude version id 13594
+					return null;
+				}
+				
+				showScheduleHtml(player, false);
+				
+				if (getQuestItemsCount(player, 57) >= BUFF_PRICE)
+				{
+					takeItems(player, 57, BUFF_PRICE);
+					applyBuffs(player);
+					startQuestTimer("summon_buff_npc", BUFF_DELAY, null, player);
+					_reuseTime.put(player.getObjectId(), System.currentTimeMillis() + (15 * 1000)); // 15 seconds waiting for the next buff
+					
+					return "buff_thank_you.html";
+				}
+				return "no_adena.html";
+			case "loadSchedule":
+				loadScheduleFromFile(); // Call the method to load the scheduling file TXT
+				break;
+			case "showSchedule":
+				showScheduleHtml(player, false);
+				break;
+			case "checkSchedule":
+				checkNpcSchedule();
+				break;
+			case "checkInstance":
+				checkPlayerInstance();
+				break;
+			
+			case "summon_buff_npc":
+				if (!playerNpcMap.containsKey(player.getObjectId()))
+				{
+					int randomNpcId = FOLLOW_BUFF_NPCS[_random.nextInt(FOLLOW_BUFF_NPCS.length)];
+					
+					Npc buffNpc = addSpawn(randomNpcId, player.getX(), player.getY() - 350, player.getZ(), 0, false, NPC_DURATION);
+					startQuestTimer("startFollowing", 500, buffNpc, player);
+					buffNpc.setTitle(player.getName() + "'s Buff NPC");
+					buffNpc.setRunning();
+					buffNpc.getAI().setIntention(CtrlIntention.AI_INTENTION_FOLLOW, player);
+					playerNpcMap.put(player.getObjectId(), buffNpc);
+					buffActive.put(player.getObjectId(), true);
+					applyBuffs(player);
+					startQuestTimer("check_status", 1000, buffNpc, player, true);
+					player.sendMessage("Talk to Follow Player or NPC Sonia and select 'Unfollow' to cancel the buffs.");
+				}
+				
+				startQuestTimer("respawn_buff_npc", BUFF_DELAY, null, player);
+				break;
+			
+			case "startFollowing":
+				npc.getAI().setIntention(CtrlIntention.AI_INTENTION_FOLLOW, player);
+				break;
+			
+			case "respawn_buff_npc":
+				if (buffActive.getOrDefault(player.getObjectId(), false))
+				{
+					Npc oldNpc = playerNpcMap.remove(player.getObjectId());
+					if (oldNpc != null)
+					{
+						oldNpc.deleteMe();
+					}
+					// Select a random NPC from the list
+					int randomNpcId = FOLLOW_BUFF_NPCS[_random.nextInt(FOLLOW_BUFF_NPCS.length)];
+					
+					Npc newNpc = addSpawn(randomNpcId, player.getX(), player.getY() - 350, player.getZ(), 0, false, NPC_DURATION);
+					newNpc.setTitle(player.getName() + "'s Buff NPC");
+					newNpc.setRunning();
+					newNpc.getAI().setIntention(CtrlIntention.AI_INTENTION_FOLLOW, player); // Follow the player
+					playerNpcMap.put(player.getObjectId(), newNpc);
+					applyBuffs(player);
+					startQuestTimer("check_status", 1000, newNpc, player, true);
+					startQuestTimer("respawn_buff_npc", BUFF_DELAY, null, player);
+				}
+				break;
+			
+			case "checkDistance":
+				for (Player p : World.getInstance().getPlayers())
+				{
+					Npc buffNpc = playerNpcMap.get(p.getObjectId());
+					if ((buffNpc != null) && (buffNpc.calculateDistance3D(p) > TELEPORT_DISTANCE))
+					{
+						// Calculates NPC's spawn position behind the player
+						int heading = p.getHeading();
+						double radian = Math.toRadians(heading);
+						int offsetX = (int) (Math.cos(radian) * -500);
+						int offsetY = (int) (Math.sin(radian) * -500);
+						int npcX = p.getX() + offsetX;
+						int npcY = p.getY() + offsetY;
+						int npcZ = p.getZ();
+						
+						buffNpc.teleToLocation(npcX, npcY, npcZ);
+						buffNpc.getAI().setIntention(CtrlIntention.AI_INTENTION_FOLLOW, p);
+					}
+				}
+				break;
+			
+			case "check_status":
+				if (shouldRemoveBuffs(player))
+				{
+					removeSupportBuffs(player);
+					if (npc != null)
+					{
+						npc.broadcastSay(ChatType.NPC_GENERAL, "Buffs removed from " + player.getName() + " due to combat condition.");
+						// Adds the 20 second lock
+						blockTimer.put(player.getObjectId(), System.currentTimeMillis() + 20000);
+					}
+				}
+				else if (buffActive.getOrDefault(player.getObjectId(), false))
+				{
+					if (blockTimer.containsKey(player.getObjectId()) && (blockTimer.get(player.getObjectId()) > System.currentTimeMillis()))
+					{
+						return null; // Does not apply the buffs if it is blocked
