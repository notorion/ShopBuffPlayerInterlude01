+	private boolean shouldRemoveBuffs(Player player)
+	{
+		boolean isPvP = (player.getPvpFlag() > 0) || (player.getReputation() < 0);
+		boolean isUsingAreaSkill = isPlayerUsingAreaSkill(player);
+		boolean isInCombatWithBoss = false;
+		boolean isInCombatWithPlayer = false;
+		
+		// Checks the player's state of combat
+		if (player.isInCombat())
+		{
+			for (WeakReference<Creature> weakRef : player.getAttackByList())
+			{
+				Creature attacker = weakRef.get();
+				if ((attacker instanceof GrandBoss) || (attacker instanceof RaidBoss))
+				{
+					isInCombatWithBoss = true;
+					break;
+				}
+			}
+			
+			if (!isInCombatWithBoss)
+			{
+				WorldObject target = player.getTarget();
+				if ((target instanceof GrandBoss) || (target instanceof RaidBoss))
+				{
+					isInCombatWithBoss = true;
+				}
+				else if (target instanceof Npc)
+				{
+					Npc npc = (Npc) target;
+					if (!npc.getTemplate().getType().equals("Monster"))
+					{
+						isInCombatWithPlayer = true;
+					}
+				}
+				else if (target instanceof Player)
+				{
+					isInCombatWithPlayer = true;
+				}
+			}
+			
+			if (!isInCombatWithBoss && !isInCombatWithPlayer)
+			{
+				for (Creature target : World.getInstance().getVisibleObjectsInRange(player, Creature.class, 1500))
+				{
+					if (target.isInCombat())
+					{
+						for (WeakReference<Creature> weakRef : target.getAttackByList())
+						{
+							Creature attacker = weakRef.get();
+							if ((attacker instanceof GrandBoss) || (attacker instanceof RaidBoss))
+							{
+								isInCombatWithBoss = true;
+								break;
+							}
+						}
+						if (isInCombatWithBoss)
+						{
+							break;
+						}
+					}
+				}
+			}
+		}
+		
+		if (isPvP || isUsingAreaSkill || isInCombatWithBoss || isInCombatWithPlayer)
+		{
+			return true;
+		}
+		
+		return false;
+	}
