+	private boolean isPlayerUsingAreaSkill(Player player)
+	{
+		SkillUseHolder queuedSkill = player.getQueuedSkill();
+		if (queuedSkill != null)
+		{
+			Skill skill = queuedSkill.getSkill();
+			if (isAreaSkill(skill))
+			{
+				return checkAreaSkillTargets(player, skill);
+			}
+		}
+		
+		if (player.isCastingNow())
+		{
+			Location skillPosition = player.getCurrentSkillWorldPosition();
+			if (skillPosition != null)
+			{
+				return checkAreaSkillTargets(player, null);
+			}
+		}
+		
+		return false;
+	}
+	
+	private boolean isAreaSkill(Skill skill)
+	{
+		return (skill != null) && ((skill.getAffectScope() == AffectScope.FAN) || (skill.getAffectScope() == AffectScope.SQUARE));
+	}
